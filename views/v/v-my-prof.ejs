            <div class="uk-flex uk-flex-middle uk-flex-center">
                <p class="uk-margin">
                    <button onclick="history.back()"
                        class="uk-text-center uk-button-large uk-button-primary uk-width-auto"><span
                            uk-icon="icon: arrow-left; ratio:1"></span>&ensp;戻る
                    </button>
                    <button onclick="location.href='./v-edit-prof?lang={{payload.lang}}'"
                        class="uk-text-center uk-button-large uk-button-primary uk-width-auto"><span
                            uk-icon="icon: file-edit; ratio:1"></span>&ensp;編集
                    </button>
                </p>
            </div>

        </div>

