<!DOCTYPE html>
<html>

<head>
    <title>c-my-prof</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/uikit/3.1.6/css/uikit.min.css" />
          <link rel="stylesheet" href="/css/common.css" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/uikit/3.1.6/js/uikit.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/uikit/3.1.6/js/uikit-icons.min.js"></script>
      <script src="/js/show-menu.js"></script>
</head>

<body>

    <!-- menu icon -->
    <script>show_i18n();</script>

    <div class="uk-padding">



        <!-- title -->
        <div class="uk-section-xsmall uk-section-primary">
            <div class="uk-text-center uk-flex-middle ">
                <div class="uk-text-large">登録情報</div>
            </div>
        </div>

        <!-- profile -->
        <div class="uk-section-xsmall uk-section-default">
            <ul class="uk-list uk-list-striped">
                <li>
                    <div class="uk-grid-divider  uk-grid-small uk-flex-middle" uk-grid>
                        <div class="uk-width-auto">
                            <div class="uk-text-break">
                                name
                            </div>
                        </div>

                        <div class="uk-width-expand">
                            <div class="uk-text-break">
                                Aichi Hanako
                            </div>
                        </div>
                    </div>
                </li>
                <li>
                    <div class="uk-grid-divider  uk-grid-small uk-flex-middle" uk-grid>
                        <div class="uk-width-auto">
                            <div class="uk-text-break">
                                age
                            </div>
                        </div>

                        <div class="uk-width-expand">
                            <div class="uk-text-break">
                                39
                            </div>
                        </div>
                    </div>
                </li>

                <li>
                    <div class="uk-grid-divider  uk-grid-small uk-flex-middle" uk-grid>
                        <div class="uk-width-auto">
                            <div class="uk-text-break">
                                居住地
                            </div>
                        </div>

                        <div class="uk-width-expand">
                            <div class="uk-text-break">
                                北海道
                            </div>
                        </div>
                    </div>
                </li>

                <li>
                    <div class="uk-grid-divider  uk-grid-small uk-flex-middle" uk-grid>
                        <div class="uk-width-auto">
                            <div class="uk-text-break">
                                専門・資格等
                            </div>
                        </div>

                        <div class="uk-width-expand">
                            <div class="uk-flex uk-flex uk-flex-left">
                                <div class="skills uk-text-break">防災士</div>
                                <div class="skills uk-text-break">治水</div>
                            </div>
                        </div>
                    </div>
                </li>

                <li>
                    <div class="uk-grid-divider  uk-grid-small uk-flex-middle" uk-grid>
                        <div class="uk-width-auto">
                            <div class="uk-text-break">
                                所属
                            </div>
                        </div>

                        <div class="uk-width-expand">
                            <div class="uk-flex uk-flex uk-flex-left">
                                <div class="uk-text-break">なになに大学</div>
                            </div>
                        </div>
                    </div>
                </li>

                <li>
                    <div class="uk-grid-divider  uk-grid-small uk-flex-middle" uk-grid>
                        <div class="uk-width-auto">
                            <div class="uk-text-break">
                                電話番号
                            </div>
                        </div>

                        <div class="uk-width-expand">
                            <div class="uk-flex uk-flex uk-flex-left">
                                <div class="uk-text-break">090-1234-5678</div>
                            </div>
                        </div>
                    </div>
                </li>

                <li>
                    <div class="uk-grid-divider  uk-grid-small uk-flex-middle" uk-grid>
                        <div class="uk-width-auto">
                            <div class="uk-text-break">
                                メールアドレス
                            </div>
                        </div>

                        <div class="uk-width-expand">
                            <div class="uk-flex uk-flex uk-flex-left">
                                <div class="uk-text-break">abc@def.com</div>
                            </div>
                        </div>
                    </div>
                </li>


            </ul>
        </div>

        <!-- Back & Edit -->
        <div class="uk-section-xsmall uk-section-muted uk-text-center">
                <div class="uk-flex uk-flex-middle uk-flex-center">
                    <p class="uk-margin">
                        <button onclick="history.back()"
                            class="uk-text-center uk-button-large uk-button-primary uk-width-auto"><span
                                uk-icon="icon: arrow-left; ratio:1"></span>&ensp; 戻る
                        </button>
                        <button onclick="location.href='./c-edit-prof'"
                            class="uk-text-center uk-button-large uk-button-primary uk-width-auto"><span
                                uk-icon="icon: file-edit; ratio:1"></span>&ensp; 編集
                        </button>
                    </p>
                </div>

            </div>



    </div>

</html>
